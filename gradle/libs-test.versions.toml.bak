[versions]
junit5 = "5.8.2"
assertJ = "3.22.0"
mockito = "4.5.1"
mockk = "1.12.4"
kotlinTestAssertions = "5.3.0"
awsMock = "2.4.10"

[libraries]

junitAPI = { module = "org.junit.jupiter:junit-jupiter-api", version.ref = "junit5" }
junitEngine = { module = "org.junit.jupiter:junit-jupiter-engine", version.ref = "junit5" }
assertJ = { module = "org.assertj:assertj-core", version.ref = "assertJ" }

mockito = { module = "org.mockito:mockito-core", version.ref = "mockito" }

mockk = { module = "io.mockk:mockk", version.ref = "mockk" }
kotlinTestAssertions = { module = "io.kotest:kotest-assertions-core-jvm", version.ref = "kotlinTestAssertions" }
awsMock = { module = "com.adobe.testing:s3mock-junit5", version.ref = "awsMock" }

